{
  "name": "@otoys/demo-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc && build:core",
    "build:core": "rm -rf src/modules/core && cd ../../ && npm run build && cp -R build/core otoys-demo/server/src/modules/core",
